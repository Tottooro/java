public class Human {
    private String name;
    private int age;
    public Human(String name, int age){
        this.age = age;
        this.name = name;
    }

    @Override
    public String toString() {
        return "Name: " + name + ".\nAge: " + age;
    }

    public String getName() {
        return name;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public void setName(String name) {
        this.name = name;
    }
}
